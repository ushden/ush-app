{
  "expo": {
    "name": "ush-app",
    "slug": "ush-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Club48.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.den48.ushapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "First app on React Native",
    "githubUrl": "https://github.com/ushden/ush-app"
  }
}
